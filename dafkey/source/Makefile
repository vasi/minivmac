ProjNm = DAFKEYmk_source_d = :mk_output_d = :mk_built_program = {mk_output_d}{ProjNm}mk_CallC = Cmk_COptions = -r -b2 -mbg offTheApplication Ä "{mk_built_program}""{mk_output_d}{ProjNm}.c.o" Ä "{mk_source_d}{ProjNm}.c"	{mk_CallC} "{mk_source_d}{ProjNm}.c" -o "{mk_output_d}{ProjNm}.c.o" {mk_COptions}"{mk_output_d}{ProjNm}.a.o" Ä "{mk_source_d}{ProjNm}.a"	ASM -case on "{mk_source_d}{ProjNm}.a" -o "{mk_output_d}{ProjNm}.a.o"ObjFiles = ¶	"{mk_output_d}{ProjNm}.a.o" ¶	"{mk_output_d}{ProjNm}.c.o""{mk_built_program}" Ä {ObjFiles}	Link -p -rn -rt FKEY=5 -t FKEY -c RSED ¶		-o "{mk_built_program}" ¶		{ObjFiles}	SetFile -d . -m . -a B "{mk_built_program}"clean Ä	Delete -i "{mk_built_program}" {ObjFiles}