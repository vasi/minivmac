C6E01, #$5D42B980	TestOneCase	#$3899E373, #$09622878, #$3899E3EB; negative overflow A	MOVEQ	#$17, D3	TestOneCase	#$0BBAF080, #$91B8C880, #$0BBAF000; negative overflow B	MOVEQ	#$13, D3	TestOneCase	#$39B61A80, #$61E24E81, #$39B61A01	TestOneCase	#$92ED0DFF, #$F54B2D80, #$92ED0D7F	TestOneCase	#$77E6DBBF, #$7E0585C0, #$77E6DB7F	TestOneCase	#$98CF79A7, #$E74DAA8B, #$98CF7932	RTS	ENDPROCAddWTest_Code	PROC	ADD.W	D6, D7	RTS	ENDPROCAddWTests	PROC	LEA	#'ADD.W Fails',A0	LEA	AddWTest_Code, A1; zero result A	MOVEQ	#$04, D3	TestOneCase	#$91BC0000, #$DB410000, #$91BC0000; zero result B	MOVEQ	#$15, D3	TestOneCase	#$68550001, #$6756FFFF, #$68550000	TestOneCase	#$C9608001, #$6ABF7FFF, #$C9600000	TestOneCase	#$C9604324, #$6ABFBCDC, #$C9600000; positive result A	MOVEQ	#$00, D3	TestOneCase	#$68850000, #$67560001, #$68850001	TestOneCase	#$67857FFF, #$68860000, #$67857FFF	TestOneCase	#$A2AA3FFF, #$FF944000, #$A2AA7FFF	TestOneCase	#$8D885328, #$03A10C34, #$8D885F5C; positive result B	MOVEQ	#$11, D3	TestOneCase	#$9A8CFFFF, #$B6FF0002, #$9A8C0001	TestOneCase	#$A8D3FFFF, #$056F7FFF, #$A8D37FFE	TestOneCase	#$FC488002, #$A06E7FFF, #$FC480001	TestOneCase	#$DE61EC34, #$4DDA3856, #$DE61248A; negative result A	MOVEQ	#$19, D3	TestOneCase	#$759AC000, #$F634C000, #$759A8000	TestOneCase	#$72EBFFFF, #$3BE6FFFF, #$72EBFFFE	TestOneCase	#$1B06FFFF, #$9D1B8001, #$1B068000	TestOneCase	#$AC58CD14, #$4F4AF783, #$AC58C497; negative result B	MOVEQ	#$08, D3	TestOneCase	#$F24F8000, #$C2C70000, #$F24F8000	TestOneCase	#$19507FFF, #$17978000, #$1950FFFF	TestOneCase	#$ECC0FFFF, #$1DD90000, #$ECC0FFFF	TestOneCase	#$59303BD7, #$0C1899C8, #$5930D59F; positive overflow	MOVEQ	#$0A, D3	TestOneCase	#$DA894000, #$94084000, #$DA898000	TestOneCase	#$19C87FFF, #$6C937FFF, #$19C8FFFE	TestOneCase	#$42B97FFF, #$4C6E0001, #$42B98000	TestOneCase	#$99E373AE, #$62287811, #$99E3EBBF; negative overflow A	MOVEQ	#$17, D3	TestOneCase	#$BAF08000, #$B8C88000, #$BAF00000; negative overflow B	MOVEQ	#$13, D3	TestOneCase	#$B61A8000, #$E24E8001, #$B61A0001	TestOneCase	#$ED0DFFFF, #$4B2D8000, #$ED0D7FFF	TestOneCase	#$E6DBBFFF, #$0585C000, #$E6DB7FFF	TestOneCase	#$CF79A73C, #$4DAA8BB2, #$CF7932EE	RTS	ENDPROCAddLTest_Code	PROC	ADD.L	D6, D7	RTS	ENDPROCAddLTests	PROC	LEA	#'ADD.L Fails',A0	LEA	AddLTest_Code, A1; zero result A	MOVEQ	#$04, D3	TestOneCase	#$00000000, #$00000000, #$00000000; zero result B	MOVEQ	#$15, D3	TestOneCase	#$00000001, #$FFFFFFFF, #$00000000	TestOneCase	#$80000001, #$7FFFFFFF, #$00000000	TestOneCase	#$4324CB21, #$BCDB34DF, #$00000000; positive result A	MOVEQ	#$00, D3	TestOneCase	#$00000000, #$00000001, #$00000001	TestOneCase	#$7FFFFFFF, #$00000000, #$7FFFFFFF	TestOneCase	#$3FFFFFFF, #$40000000, #$7FFFFFFF	TestOneCase	#$53283F45, #$0C3424DC, #$5F5C6421; positive result B	MOVEQ	#$11, D3	TestOneCase	#$FFFFFFFF, #$00000002, #$00000001	TestOneCase	#$FFFFFFFF, #$7FFFFFFF, #$7FFFFFFE	TestOneCase	#$80000002, #$7FFFFFFF, #$00000001	TestOneCase	#$EC34C428, #$38569D33, #$248B615B; negative result A	MOVEQ	#$19, D3	TestOneCase	#$C0000000, #$C0000000, #$80000000	TestOneCase	#$FFFFFFFF, #$FFFFFFFF, #$FFFFFFFE	TestOneCase	#$FFFFFFFF, #$80000001, #$80000000	TestOneCase	#$CD145589, #$F783A12A, #$C497F6B3; negative result B	MOVEQ	#$08, D3	TestOneCase	#$80000000, #$00000000, #$80000000	TestOneCase	#$7FFFFFFF, #$80000000, #$FFFFFFFF	TestOneCase	#$FFFFFFFF, #$00000000, #$FFFFFFFF	TestOneCase	#$3BD7C4C9, #$99C85D7D, #$D5A02246; positive overflow	MOVEQ	#$0A, D3	TestOneCase	#$40000000, #$40000000, #$80000000	TestOneCase	#$7FFFFFFF, #$7FFFFFFF, #$FFFFFFFE	TestOneCase	#$7FFFFFFF, #$00000001, #$80000000	TestOneCase	#$73AE4387, #$7811CDFE, #$EBC01185; negative overflow A	MOVEQ	#$17, D3	TestOneCase	#$80000000, #$80000000, #$00000000; negative overflow B	MOVEQ	#$13, D3	TestOneCase	#$80000000, #$80000001, #$00000001	TestOneCase	#$FFFFFFFF, #$80000000, #$7FFFFFFF	TestOneCase	#$BFFFFFFF, #$C0000000, #$7FFFFFFF	TestOneCase	#$A73C1113, #$8BB29780, #$32EEA893	RTS	ENDPROCAddTests PROC EXPORT	MOVE.B	#$0, D5	JSR	AddBTests	JSR	AddWTests	JSR	AddLTests	RTS	ENDPROC		END