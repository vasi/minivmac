07FFF, #$6ABF7FFF, #$C9607FFF	TestOneCase	#$E5711A0D, #$162462CB, #$E5710209	TestOneCase	#$E70FFFFF, #$FCA80001, #$E70F0001	TestOneCase	#$E70F8BD0, #$FCA8134B, #$E70F0340	TestOneCase	#$67462D45, #$68458845, #$67460845; zero result	MOVEQ	#$04, D3	TestOneCase	#$68850000, #$67560000, #$68850000	TestOneCase	#$67850000, #$68867FFF, #$67850000	TestOneCase	#$A2AA1764, #$FF94600B, #$A2AA0000	TestOneCase	#$8D888000, #$03A10000, #$8D880000	TestOneCase	#$9D888000, #$44EB7FFF, #$9D880000	TestOneCase	#$E7A1FFFF, #$AAC00000, #$E7A10000	TestOneCase	#$E7A1B160, #$AAC04217, #$E7A10000	TestOneCase	#$24402316, #$2C8DD4A1, #$24400000; negative result	MOVEQ	#$08, D3	TestOneCase	#$EC62FFFF, #$0870FFFF, #$EC62FFFF	TestOneCase	#$18EAFFFF, #$2DBF8000, #$18EA8000	TestOneCase	#$9A8C8000, #$B6FF8000, #$9A8C8000	TestOneCase	#$A8D39A35, #$056FBF0E, #$A8D39A04	TestOneCase	#$FC48AE69, #$A06E9E44, #$FC488E40	RTS	ENDPROCAndLTest_Code	PROC	AND.L	D6, D7	RTS	ENDPROCAndLTests	PROC	LEA	#'AND.L Fails',A0	LEA	AndLTest_Code, A1; positive result	MOVEQ	#$00, D3	TestOneCase	#$00000001, #$00000001, #$00000001	TestOneCase	#$7FFFFFFF, #$00000001, #$00000001	TestOneCase	#$7FFFFFFF, #$7FFFFFFF, #$7FFFFFFF	TestOneCase	#$1A0DE571, #$62CB1624, #$02090420	TestOneCase	#$FFFFFFFF, #$00000001, #$00000001	TestOneCase	#$8BD0E70F, #$134BFCA8, #$0340E408	TestOneCase	#$2D456746, #$88456845, #$08456044; zero result	MOVEQ	#$04, D3	TestOneCase	#$00000000, #$00000000, #$00000000	TestOneCase	#$00000000, #$7FFFFFFF, #$00000000	TestOneCase	#$17648469, #$600B7A94, #$00000000	TestOneCase	#$80000000, #$00000000, #$00000000	TestOneCase	#$80000000, #$7FFFFFFF, #$00000000	TestOneCase	#$FFFFFFFF, #$00000000, #$00000000	TestOneCase	#$B160E7A1, #$42171844, #$00000000	TestOneCase	#$23162440, #$D4A1189D, #$00000000; negative result	MOVEQ	#$08, D3	TestOneCase	#$FFFFFFFF, #$FFFFFFFF, #$FFFFFFFF	TestOneCase	#$FFFFFFFF, #$80000000, #$80000000	TestOneCase	#$80000000, #$80000000, #$80000000	TestOneCase	#$9A35A8D3, #$BF0E056F, #$9A040043	TestOneCase	#$AE69FC48, #$9E44A06E, #$8E40A048	RTS	ENDPROCAndTests PROC EXPORT	MOVE.B	#$10, D5	JSR	AndBTests	JSR	AndWTests	JSR	AndLTests	RTS	ENDPROC		END