2	BCLR	#2, D1	MOVE	D1, CCR	BEQ	@3	RTS	ENDPROCBPLTests	PROC	BCLR	#0, D1	BSR	@6	BSET	#0, D1@6	BCLR	#2, D1	BSR	@5	BSET	#2, D1@5	BCLR	#1, D1	BSR	@4	BSET	#1, D1@4	BCLR	#3, D1	MOVE	D1, CCR	BPL	@2@3	LEA	#'BPL fails',A0	JMP	MsgExit@2	BSET	#3, D1	MOVE	D1, CCR	BPL	@3	RTS	ENDPROCBMITests	PROC	BCLR	#0, D1	BSR	@6	BSET	#3, D1@6	BCLR	#0, D1	BSR	@5	BSET	#2, D1@5	BCLR	#1, D1	BSR	@4	BSET	#1, D1@4	BSET	#3, D1	MOVE	D1, CCR	BMI	@2@3	LEA	#'BMI fails',A0	JMP	MsgExit@2	BCLR	#3, D1	MOVE	D1, CCR	BMI	@3	RTS	ENDPROCBHITests	PROC	BCLR	#3, D1	BSR	@6	BSET	#3, D1@6	BCLR	#1, D1	BSR	@4	BSET	#1, D1@4	BCLR	#0, D1	BCLR	#2, D1	MOVE	D1, CCR	BHI	@2@3	LEA	#'BHI fails',A0	JMP	MsgExit@2	BSET	#0, D1	MOVE	D1, CCR	BHI	@3	BSET	#2, D1	MOVE	D1, CCR	BHI	@3	BCLR	#0, D1	MOVE	D1, CCR	BHI	@3	RTS	ENDPROCBLSTests	PROC	BCLR	#3, D1	BSR	@6	BSET	#3, D1@6	BCLR	#1, D1	BSR	@4	BSET	#1, D1@4	BSET	#0, D1	BSET	#2, D1	MOVE	D1, CCR	BLS	@2@3	LEA	#'BLS fails',A0	JMP	MsgExit@2	BCLR	#2, D1	MOVE	D1, CCR	BLS	@7	BRA	@3@7	BCLR	#0, D1	MOVE	D1, CCR	BLS	@3	BSET	#2, D1	MOVE	D1, CCR	BLS	@8	BRA	@3@8	RTS	ENDPROCBGETests	PROC	BCLR	#0, D1	BSR	@5	BSET	#0, D1@5	BCLR	#2, D1	BSR	@4	BSET	#2, D1@4	BCLR	#1, D1	BCLR	#3, D1	MOVE	D1, CCR	BGE	@2@3	LEA	#'BGE fails',A0	JMP	MsgExit@2	BSET	#1, D1	MOVE	D1, CCR	BGE	@3	BSET	#3, D1	MOVE	D1, CCR	BGE	@7	BRA	@3@7	BCLR	#1, D1	MOVE	D1, CCR	BGE	@3	RTS	ENDPROCBLTTests	PROC	BCLR	#0, D1	BSR	@5	BSET	#0, D1@5	BCLR	#2, D1	BSR	@4	BSET	#2, D1@4	BCLR	#1, D1	BCLR	#3, D1	MOVE	D1, CCR	BLT	@3	BSET	#1, D1	MOVE	D1, CCR	BLT	@2@3	LEA	#'BLT fails',A0	JMP	MsgExit@2	BSET	#3, D1	MOVE	D1, CCR	BLT	@3	BCLR	#1, D1	MOVE	D1, CCR	BLT	@7	BRA	@3@7	RTS	ENDPROCBGTTests	PROC	BCLR	#0, D1	BSR	@4	BSET	#0, D1@4	BCLR	#1, D1	BCLR	#2, D1	BCLR	#3, D1	MOVE	D1, CCR	BGT	@2@3	LEA	#'BGT fails',A0	JMP	MsgExit@2	BSET	#1, D1	BCLR	#2, D1	BSET	#3, D1	MOVE	D1, CCR	BGT	@5	BRA	@3@5	BCLR	#1, D1	BCLR	#2, D1	BSET	#3, D1	MOVE	D1, CCR	BGT	@3	BSET	#2, D1	BCLR	#3, D1	MOVE	D1, CCR	BGT	@3	BSET	#3, D1	MOVE	D1, CCR	BGT	@3	BSET	#1, D1	BCLR	#2, D1	BCLR	#3, D1	MOVE	D1, CCR	BGT	@3	BSET	#2, D1	MOVE	D1, CCR	BGT	@3	BSET	#3, D1	MOVE	D1, CCR	BGT	@3	RTS	ENDPROCBLETests	PROC	BCLR	#0, D1	BSR	@4	BSET	#0, D1@4	BCLR	#1, D1	BCLR	#2, D1	BCLR	#3, D1	MOVE	D1, CCR	BLE	@3	BSET	#1, D1	BCLR	#2, D1	BSET	#3, D1	MOVE	D1, CCR	BLE	@3	BCLR	#1, D1	BCLR	#2, D1	BSET	#3, D1	MOVE	D1, CCR	BLE	@2@3	LEA	#'BLE fails',A0	JMP	MsgExit@2	BSET	#2, D1	BCLR	#3, D1	MOVE	D1, CCR	BLE	@5	BRA	@3@5	BSET	#3, D1	MOVE	D1, CCR	BLE	@6	BRA	@3@6	BSET	#1, D1	BCLR	#2, D1	BCLR	#3, D1	MOVE	D1, CCR	BLE	@7	BRA	@3@7	BSET	#2, D1	MOVE	D1, CCR	BLE	@8	BRA	@3@8	BSET	#3, D1	MOVE	D1, CCR	BLE	@9	BRA	@3@9	RTS	ENDPROCBrCondTests PROC EXPORT	MOVEQ	#0, D1	BCLR	#4, D1	BSR	@2	BSET	#4, D1@2	JSR	BCCTests	JSR	BCSTests	JSR	BVCTests	JSR	BVSTests	JSR	BNETests	JSR	BEQTests	JSR	BPLTests	JSR	BMITests	JSR	BHITests	JSR	BLSTests	JSR	BGETests	JSR	BLTTests	JSR	BGTTests	JSR	BLETests	RTS	ENDPROC		END