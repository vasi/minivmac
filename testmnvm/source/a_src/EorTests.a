FFFFE, #$FCA8FFFF, #$E70F0001	TestOneCase	#$67468001, #$68458000, #$67460001 	TestOneCase	#$12468D87, #$68458443, #$124609C4; zero result	MOVEQ	#$04, D3	TestOneCase	#$68858000, #$67568000, #$68850000	TestOneCase	#$67857FFF, #$68867FFF, #$67850000	TestOneCase	#$A2AA0000, #$FF940000, #$A2AA0000	TestOneCase	#$8D882567, #$03A12567, #$8D880000; negative result	MOVEQ	#$08, D3	TestOneCase	#$EC627FFF, #$08708000, #$EC62FFFF	TestOneCase	#$18EAFFFF, #$2DBF0000, #$18EAFFFF	TestOneCase	#$9A8C8000, #$B6FF0000, #$9A8C8000	TestOneCase	#$A8D3FFFF, #$056F7FFF, #$A8D38000	TestOneCase	#$FC48C969, #$A06E6C44, #$FC48A52D	RTS	ENDPROCEorLTest_Code	PROC	EOR.L	D6, D7	RTS	ENDPROCEorLTests	PROC	LEA	#'EOR.L Fails',A0	LEA	EorLTest_Code, A1; positive result	MOVEQ	#$00, D3	TestOneCase	#$7FFFFFFF, #$00000000, #$7FFFFFFF	TestOneCase	#$7FFFFFFF, #$7FFFFFFE, #$00000001	TestOneCase	#$00000000, #$00000001, #$00000001	TestOneCase	#$0E0DE571, #$29CB1624, #$27C6F355	TestOneCase	#$FFFFFFFF, #$80000000, #$7FFFFFFF	TestOneCase	#$FFFFFFFE, #$FFFFFFFF, #$00000001	TestOneCase	#$80000001, #$80000000, #$00000001 	TestOneCase	#$8D871246, #$84436845, #$09C47A03; zero result	MOVEQ	#$04, D3	TestOneCase	#$80000000, #$80000000, #$00000000	TestOneCase	#$7FFFFFFF, #$7FFFFFFF, #$00000000	TestOneCase	#$00000000, #$00000000, #$00000000	TestOneCase	#$25678D88, #$25678D88, #$00000000; negative result	MOVEQ	#$08, D3	TestOneCase	#$7FFFFFFF, #$80000000, #$FFFFFFFF	TestOneCase	#$FFFFFFFF, #$00000000, #$FFFFFFFF	TestOneCase	#$80000000, #$00000000, #$80000000	TestOneCase	#$FFFFFFFF, #$7FFFFFFF, #$80000000	TestOneCase	#$C969FC48, #$6C44A06E, #$A52D5C26	RTS	ENDPROCEorTests PROC EXPORT	MOVE.B	#$10, D5	JSR	EorBTests	JSR	EorWTests	JSR	EorLTests	RTS	ENDPROC		END