COrLTests	PROC	LEA	#'OR.L Fails',A0	LEA	OrLTest_Code, A1; positive result	MOVEQ	#$00, D3	TestOneCase	#$7FFFFFFF, #$00000000, #$7FFFFFFF	TestOneCase	#$7FFFFFFF, #$7FFFFFFF, #$7FFFFFFF	TestOneCase	#$00000000, #$00000001, #$00000001	TestOneCase	#$190DE571, #$3ECB1624, #$3FCFF775; zero result	MOVEQ	#$04, D3	TestOneCase	#$00000000, #$00000000, #$00000000; negative result	MOVEQ	#$08, D3	TestOneCase	#$FFFFFFFF, #$00000000, #$FFFFFFFF	TestOneCase	#$FFFFFFFF, #$80000001, #$FFFFFFFF	TestOneCase	#$7FFFFFFF, #$80000000, #$FFFFFFFF	TestOneCase	#$80000000, #$80000000, #$80000000	TestOneCase	#$9969FC48, #$CB44A06E, #$DB6DFC6E	RTS	ENDPROCOrTests PROC EXPORT	MOVE.B	#$10, D5	JSR	OrBTests	JSR	OrWTests	JSR	OrLTests	RTS	ENDPROC		END